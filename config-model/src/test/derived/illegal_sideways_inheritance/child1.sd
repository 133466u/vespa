schema child1 {

  document child1 {
    field field1 type int {
      indexing: attribute
    }

  }

  rank-profile child inherits parent {
    function function2() {
      expression: attribute(field1) + 5
    }

    first-phase {
      expression: function2() * function1()
    }

    summary-features {
      function1
      function2
      attribute(field1)
    }
  }
}
